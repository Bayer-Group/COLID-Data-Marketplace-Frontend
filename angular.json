{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "colid-data-marketplace-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/colid-data-marketplace-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": ["src/assets"],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss",
              "./node_modules/quill/dist/quill.core.css",
              "./node_modules/quill/dist/quill.bubble.css",
              "./node_modules/quill/dist/quill.snow.css",
              "./node_modules/bootstrap/dist/css/bootstrap.css"
            ],
            "scripts": ["./node_modules/marked/marked.min.js"],
            "outputHashing": "none",
            "aot": true,
            "optimization": false,
            "vendorChunk": true,
            "buildOptimizer": false,
            "namedChunks": true,
            "extractLicenses": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.production.ts"
                }
              ],
              "outputHashing": "all",
              "aot": true,
              "optimization": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "namedChunks": true,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12kb",
                  "maximumError": "20kb"
                }
              ]
            },
            "es5": {
              "tsConfig": "src/tsconfig.app.json"
            },
            "release": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.release.ts"
                }
              ],
              "outputHashing": "all",
              "aot": true,
              "optimization": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "namedChunks": true,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12kb",
                  "maximumError": "20kb"
                }
              ]
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ],
              "outputHashing": "all",
              "aot": true,
              "optimization": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "namedChunks": true,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12kb",
                  "maximumError": "20kb"
                }
              ]
            },
            "docker": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker.ts"
                }
              ],
              "outputHashing": "all",
              "aot": true,
              "optimization": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "namedChunks": true,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12kb",
                  "maximumError": "20kb"
                }
              ]
            },
            "onprem": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.onprem.ts"
                }
              ],
              "outputHashing": "all",
              "aot": true,
              "optimization": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "namedChunks": true,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12kb",
                  "maximumError": "20kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "host": "0.0.0.0",
            "port": 4301,
            "buildTarget": "colid-data-marketplace-frontend:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "colid-data-marketplace-frontend:build:production"
            },
            "es5": {
              "tsConfig": "src/tsconfig.app.json"
            },
            "release": {
              "buildTarget": "colid-data-marketplace-frontend:build:release"
            },
            "development": {
              "buildTarget": "colid-data-marketplace-frontend:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "colid-data-marketplace-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": ["src/favicon.ico", "src/assets"],
            "codeCoverage": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    },
    "colid-data-marketplace-frontend-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "colid-data-marketplace-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "colid-data-marketplace-frontend:serve:production"
            },
            "release": {
              "devServerTarget": "colid-data-marketplace-frontend:serve:release"
            },
            "development": {
              "devServerTarget": "colid-data-marketplace-frontend:serve:development"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "1d2188cb-e97c-41ea-9f7b-4a8feff51d9c"
  }
}
